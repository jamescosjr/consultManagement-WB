{
  "id": null,
  "title": "ConsultManagement API Overview",
  "tags": ["api", "node", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests per second",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum by (method, route) (rate(http_request_duration_ms_count[1m]))",
          "legendFormat": "{{method}} {{route}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (ms)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, route, method) (rate(http_request_duration_ms_bucket[5m])))",
          "legendFormat": "{{method}} {{route}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error rate (4xx/5xx rps)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_ms_count{status_code=~\"4..\|5..\"}[1m]))",
          "legendFormat": "errors rps"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Process memory (bytes)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "rss"
        }
      ]
    }
  ]
}
